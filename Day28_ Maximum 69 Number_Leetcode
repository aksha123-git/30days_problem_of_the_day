class Solution {
public:
    int maximum69Number (int num) {
        string s = to_string(num);//easy to manipulate string as to array
        for(int i =0; i<s.size(); i++){
            if(s[i] == '6'){
                s[i] = '9';
                break;
            }
        }
        return stoi(s);//convert the integer component of a string to an integer type. 
        
    }
};
