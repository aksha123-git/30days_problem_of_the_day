class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
    
        // we add both string and check if they are equal or not
        string w1 = "";
        string w2 = "";
        for( auto s : word1)
            w1 = w1 +s;
        for( auto s : word2)
            w2 = w2 +s;
        return w1 == w2;
        
    }
};
